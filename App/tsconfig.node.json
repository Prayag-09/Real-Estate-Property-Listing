{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "jsx": "react-jsx",
    "lib": [
      "ES2015",
      "DOM"
    ],
  },
  "include": [
    "src/**/*",
    "vite.config.ts"
  ]
}